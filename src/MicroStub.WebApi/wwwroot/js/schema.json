{
  "title": "StubConfig",
  "type": "object",
  "properties": {
    "Subscribers": {
      "format": "tabs",
      "required": true,
      "type": "array",
      "items": {
        "headerTemplate": "{{self.Key}}",
        "type": "object",
        "properties": {
          "Key": {
            "required": true,
            "type": "string"
          },
          "Secret": {
            "required": true,
            "type": "string"
          },
          "Projects": {
            "format": "tabs",
            "required": true,
            "type": "array",
            "items": {
              "headerTemplate": "{{self.Name}}",
              "type": "object",
              "properties": {
                "Name": {
                  "required": true,
                  "type": "string"
                },
                "EndPoints": {
                  "format": "tabs",
                  "required": true,
                  "type": "array",
                  "items": {
                    "headerTemplate": "{{self.Address}}",
                    "type": "object",
                    "properties": {
                      "Address": {
                        "required": true,
                        "type": "string"
                      },
                      "Methods": {
                        "format": "tabs",
                        "required": true,
                        "type": "array",
                        "items": {
                          "headerTemplate": "{{self.HttpVerb}}-{{self.HttpStatusCode}}",
                          "type": "object",
                          "properties": {
                            "HttpVerb": {
                              "type": "array",
                              "format": "select",
                              "uniqueItems": true,
                              "items": {
                                "type": "string",
                                "enum": [ "GET", "POST", "PUT", "DELETE", "PATCH" ]
                              }
                            },
                            "QueryString": {
                              "required": true,
                              "type": "string"
                            },
                            "RequestBody": {
                              "format": "textarea",
                              "required": true,
                              "type": "string"
                            },
                            "ContentType": {
                              "required": true,
                              "type": "string"
                            },
                            "Response": {
                              "format": "textarea",
                              "required": true,
                              "type": "string"
                            },
                            "HttpStatusCode": {
                              "required": true,
                              "type": "integer"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}